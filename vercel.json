{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --legacy-peer-deps"
      }
    }
  ],
  "routes": [
    {
      "src": "/campaigns",
      "dest": "/campaigns"
    },
    {
      "src": "/campaigns/(.*)",
      "dest": "/campaigns/$1"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
